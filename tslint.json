{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "jsRules": true,
  "rules": {
    "align": [
      true,
      "parameters",
      "statements",
      "members",
      "elements"
    ],
    "curly": true,
    "import-name": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "jsx-space-before-trailing-slash": true,
    "max-line-length": {
      "options": [
        120
      ]
    },
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "public-instance-field"
        ],
        "alphabetize": true
      }
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": true,
    "no-console": false,
    "no-var-requires": false,
    "object-literal-sort-keys": true,
    "object-shorthand-properties-first": false,
    "only-arrow-functions": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ter-padded-blocks": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "exports": "always",
          "functions": "never",
          "imports": "always",
          "typeLiterals": "ignore"
        },
        "singleline": "never",
        "esSpecCompliant": true
      }
    ],
    "variable-name": false
  }
}